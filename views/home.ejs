<!DOCTYPE html>
<html> 
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" />
    <style>
      .child { display: none; }
      .member { font-weight: bold; }
      .arrow { text-align: center; } 
      .filters { display: none; font-weight: bold; vertical-align: middle !important; }
    </style>
  </head>
  <body>
    <div class="container">
      <h1>Members</h1>
      <table class="table" data-members="<%= JSON.stringify(members) %>">
        <thead class="members-table-head">
          <tr class="flters">
            <th></th>
            <th>Name</th> 
            <th>MemberClass</th> 
            <th>MemberCode</th> 
            <th>ObjectType</th> 
            <th>SubsystemCode</th> 
          </tr> 
        </thead> 
        <tbody class="members-table-body">
        </tbody>
      </table>
    </div>
    <script type="text/template" id="member-template">
      <td class="arrow">
        {% if (!_.isEmpty(children)) { %}
        <span class="glyphicon glyphicon-triangle-top"></span>
        {% } %}
      </td>
      <td>{{ name }}</td>
      <td>{{ memberClass }}</td>
      <td>{{ memberCode }}</td>
      <td>{{ objectType }}</td>
      <td></td>
    </script>
    <script type="text/template" id="child-template">
      <td class="arrow"></td>
      <td>{{ name }}</td>
      <td>{{ memberClass }}</td>
      <td>{{ memberCode }}</td>
      <td>{{ objectType }}</td>
      <td>{{ subsystemCode }}</td>
    </script>
    <script type="text/template" id="filters-template">
      {% if (filtering) { %}
      <tr class="filters">
        <th></th>
        <th><input type="text" placeholder="Name"></th>
        <th><input type="text" placeholder="MemberClass"></th>
        <th><input type="text" placeholder="MemberCode"></th>
        <th><input type="text" placeholder="ObjectType"></th>
        <th><input type="text" placeholder="SubsystemCode"></th>
      </tr>
      {% } else { %}
      <tr class="filters">
        <th></th>
        <th>Name</th> 
        <th>memberClass</th> 
        <th>memberCode</th> 
        <th>objectType</th> 
        <th>subsystemCode</th> 
      </tr> 
      {% } %}
    </script>
    <script type="text/javascript" src="/js/bundle.js"></script>
    <script type="text/javascript" src="//code.jquery.com/jquery-2.1.4.min.js"></script>
    <script type="text/javascript" src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
  </body>
</html>
